<!-- These resources are around just to allow their values to be customized
     for different OnePlus serires/devices.  Do not translate. -->
<resources>

    <!-- DC-dimming sysfs node path -->
    <string name="node_dc_mode_switch">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/dimlayer_bl_en</string>

    <!-- Ear gain sysfs node path -->
    <string name="node_ear_gain_preference">/sys/kernel/sound_control/earpiece_gain</string>

    <!-- High Brightnes sysfs node path -->
    <string name="node_hbm_mode_switch">/sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/card0-DSI-1/hbm</string>

    <!-- Measured FPS sysfs node path -->
    <string name="node_fps_info_service">/sys/class/drm/sde-crtc-0/measured_fps</string>

    <!-- Mic gain sysfs node path -->
    <string name="node_mic_gain_preference">/sys/kernel/sound_control/mic_gain</string>

    <!-- OTG sysfs node path -->
    <string name="otg_mode_switch">/sys/class/power_supply/usb/otg_switch</string>

    <!-- Adreno GPU Throttling sysfs node path -->
    <string name="node_gpu_throttling_mode_switch">/sys/devices/platform/soc/5000000.qcom,kgsl-3d0/kgsl/kgsl-3d0/throttling</string>

    <!-- Power Efficient Workqueue sysfs node path -->
    <string name="node_power_efficient_workqueue_mode_switch">/sys/module/workqueue/parameters/power_efficient</string>

    <!-- Call vibration sysfs node path -->
    <string name="node_vibrator_call_strength_preference">/sys/devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-03/c440000.qcom,spmi:qcom,pmi8998@3:qcom,haptics@c000/leds/vibrator/vmax_mv_call</string>

    <!-- Notif vibration sysfs node path -->
    <string name="node_vibrator_notif_strength_preference">/sys/devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-03/c440000.qcom,spmi:qcom,pmi8998@3:qcom,haptics@c000/leds/vibrator/vmax_mv_strong</string>

    <!-- System vibration sysfs node path -->
    <string name="node_vibrator_strength_preference">/sys/devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-03/c440000.qcom,spmi:qcom,pmi8998@3:qcom,haptics@c000/leds/vibrator/vmax_mv_user</string>
</resources>
