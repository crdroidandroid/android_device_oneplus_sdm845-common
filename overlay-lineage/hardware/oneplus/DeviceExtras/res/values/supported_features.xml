<!-- These resources are around just to allow their values to be customized
     for different OnePlus serires/devices.  Do not translate. -->
<resources>

    <!-- Whether device supports Alert Slider -->
    <bool name="config_deviceSupportsAlertSlider">true</bool>

    <!-- Whether device supports DC-dimming -->
    <bool name="config_deviceSupportsDCdimming">false</bool>

    <!-- Whether device supports Dolby -->
    <bool name="config_deviceSupportsDolby">false</bool>

    <!-- Whether device supports Earpiece gain -->
    <bool name="config_deviceSupportsEarGain">true</bool>

    <!-- Whether device supports FPS overlay -->
    <bool name="config_deviceSupportsFPS">true</bool>

    <!-- Whether device supports GameMode -->
    <bool name="config_deviceSupportsGameMode">false</bool>

    <!-- Whether device supports High Brightness Mode -->
    <bool name="config_deviceSupportsHBM">true</bool>

    <!-- Whether to show Jitter Test -->
    <bool name="config_deviceSupportsJitterTest">true</bool>

    <!-- Whether device supports Kcal -->
    <bool name="config_deviceSupportsKcal">false</bool>

    <!-- Whether device supports Microphone gain -->
    <bool name="config_deviceSupportsMicGain">true</bool>

    <!-- Whether device supports OTG -->
    <bool name="config_deviceSupportsOTG">false</bool>

    <!-- Whether device supports Panel Modes -->
    <bool name="config_deviceSupportsPanelModes">false</bool>

    <!-- Whether device supports Touch Screen Gestures -->
    <bool name="config_deviceSupportsTouchScreenGestures">false</bool>

    <!-- Whether device supports TP Edge Limit -->
    <bool name="config_deviceSupportsTPEdgeLimit">false</bool>

    <!-- Whether device supports USB2.0 Force Fastcharge -->
    <bool name="config_deviceSupportsUSB2FC">false</bool>

    <!-- Whether device supports System vibration intensity -->
    <bool name="config_deviceSupportsSysVib">true</bool>

    <!-- Whether device supports Notifcation vibration intensity -->
    <bool name="config_deviceSupportsNotifVib">true</bool>

    <!-- Whether device supports Call vibration intensity -->
    <bool name="config_deviceSupportsCallVib">true</bool>

    <!-- Whether device supports wireless PowerShare -->
    <bool name="config_deviceSupportsPowerShare">false</bool>

    <!-- Whether device supports wireless Charging -->
    <bool name="config_deviceSupportsWirelessCharging">false</bool>
</resources>
